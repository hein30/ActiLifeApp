<?xml version="1.0" encoding="UTF-8"?>

<ivy-module version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
            xsi:noNamespaceSchemaLocation="http://ant.apache.org/ivy/schemas/ivy.xsd">
    <info
            organisation=""
            module="ActiLifeApp"
            status="integration">
    </info>

    <configurations>
        <conf name="compile" visibility="public"/>
        <conf name="test" visibility="private"/>
    </configurations>

    <dependencies>
        <!-- https://mvnrepository.com/artifact/org.apache.poi/poi -->
        <dependency org="org.apache.poi" name="poi" rev="3.17"/>

        <!-- https://mvnrepository.com/artifact/org.apache.poi/poi-ooxml -->
        <dependency org="org.apache.poi" name="poi-ooxml" rev="3.17"/>

        <!-- https://mvnrepository.com/artifact/org.apache.velocity/velocity -->
        <dependency org="org.apache.velocity" name="velocity" rev="1.7"/>

        <!-- https://mvnrepository.com/artifact/net.imagej/ij -->
        <dependency org="net.imagej" name="ij" rev="1.51r"/>

        <!-- https://mvnrepository.com/artifact/org.apache.commons/commons-io -->
        <dependency org="org.apache.commons" name="commons-io" rev="1.3.2"/>

    </dependencies>
</ivy-module>
